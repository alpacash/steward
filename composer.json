{
    "name": "alpacash/steward",
    "homepage": "https://stew.sh",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "laravel-zero/framework": "5.8.*",
        "symfony/process": "^4.3"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["steward"]
}
